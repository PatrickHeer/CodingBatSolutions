public boolean only14(int[] nums) {
  boolean allOnesAndFours = true;
  int count = 0;
  for (int i = 0; i < nums.length; i++){
    if (nums[i] != 1 && nums[i] != 4){
      allOnesAndFours = false;
    } else {count += 1;}
    if (count == nums.length){
      allOnesAndFours = true;
    }
  }return allOnesAndFours;
}
