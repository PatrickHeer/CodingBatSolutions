public boolean more14(int[] nums) {
  int ones = 0;
  int fours = 0;
  boolean oneOverFour = false;
  for (int i = 0; i < nums.length; i++){
    if (nums[i] == 1){
      ones += 1;}
    if (nums[i] == 4){
      fours += 1;}
    if (ones > fours){
      oneOverFour = true;
    } else {oneOverFour = false;}
    } return oneOverFour;
}

